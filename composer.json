{
    "name": "karim1999/laravel-inertia-admin",
    "description": "An Admin Dashboard for Laravel Created using React and Inertia js",
    "license": "MIT",
    "keywords": [
        "karim1999",
        "laravel",
        "inertia-admin"
    ],
    "homepage": "https://github.com/karim1999/laravel-inertia-admin",
    "authors": [
        {
            "name": "Karim Abdelgwad",
            "email": "karim.elbadry2@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^6.21"
    },
    "autoload": {
        "psr-4": {
            "Karim\\LaravelInertiaAdmin\\": "src/",
            "Karim\\LaravelInertiaAdmin\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karim\\LaravelInertiaAdmin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Karim\\LaravelInertiaAdmin\\LaravelInertiaAdminServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Karim\\LaravelInertiaAdmin\\LaravelInertiaAdminFacade"
            }
        }
    },
    "require": {
        "illuminate/contracts": "^8.63",
        "spatie/laravel-package-tools": "^1.9"
    }
}
